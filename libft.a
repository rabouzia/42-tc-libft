void *memcpy(void *d, const void * s, size_t size)
{
    unsigned int i;
    char                *dest;
    const char           *src;

    if (s == NULL || d == NULL)
        return (NULL);
    dest = d;
    src = s;
    i = 0;
    while (i < size)
    {
        dest[i] = src[i];
        i++;
    }
    return (d);

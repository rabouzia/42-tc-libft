char *ft_strnstr(const char *str, const char *to_find, size_t n)
{ 
    int i;
    int j;
    
    i = 0;
    j = 0;
    if (to_find[j] == '\0')
        return (str);
    while(str[i])
    {
        while(str[i + j] && to_find[j] == str[i + j])
            j++;
        if (to_find[j] == '\0')
            return (str + i);
        i++;
        j = 0;
    }
    return (NULL);

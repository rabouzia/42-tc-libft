#include "libft.h"

char *empty(void)
{
    char *err;

    err = malloc(1);
    if(!err)
        return (NULL);
    err[0] = 0;
    return (err);
}

char *ft_substr(const char *s, unsigned int start, size_t len)
{
    size_t n;
    char *res;

    n = ft_strlen(s);
    if(!s)
        return (NULL);
    if (start > (unsigned int)n)
        return (empty());
    if (len > n - start)
        len = n - start;
    res = (char *) malloc((len + 1) *sizeof(*s));
    if (!res)
        return NULL;
    ft_strlcpy(res, s + start, len + 1);
    return (res);
}

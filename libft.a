void *ft_memmove(void *d, const void *s, size_t size)
{
    char    *result;
    
    if (s == NULL || d == NULL)
        return (NULL);
    if (!(result = (char*) malloc(sizeof(char *)* size)))
        return (NULL);
    result = ft_strncpy(result, s, size);
    d = (d, result, size);
    free(result);
    return(result);
